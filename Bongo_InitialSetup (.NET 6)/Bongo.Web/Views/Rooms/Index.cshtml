@model IEnumerable<StudyRoom>

<div class="d-flex aligns-items-center justify-content-center card my-5">
    <div class="card text-center bg-light">
        <p class="text-black display-4"> Room List</p>
    </div>

    <div class="card-body">
                @if (Model.Count() > 0)
                {
                    <table class="table table-striped border">
                        <tr class="table-secondary">
                            <th>
                                Id
                            </th>
                            <th>
                                Room Number
                            </th>
                            <th>
                                Room Name
                            </th>
                   
                        </tr>
                        @foreach (var item in Model)
                        {
                    <tr>
                        <td>
                            @item.Id
                        </td>
                        <td>
                            @item.RoomNumber
                        </td>
                        <td>
                            @item.RoomName
                        </td>
                
                    </tr>
                        }
                    </table>
                }
                else
                {
                    <p>No category exists...</p>
                }
            </div>
    </div>
</div>